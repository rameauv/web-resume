{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "ts-mocha \"src/test/**/*.spec.ts\" --recursive --exit",
    "test-debug": "ts-mocha --inspect-brk \"src/test/**/*.spec.ts\" --recursive --exit",
    "coverage": "cross-env NODE_PATH=./src nyc npm run test"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "coverage/*",
      "dist/*"
    ],
    "reporter": [
      "text",
      "HTML"
    ],
    "all": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.7",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.4.1",
    "morphism": "^1.12.3",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.16",
    "swagger-jsdoc": "^3.5.0"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.107.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.7",
    "@types/convict": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.13",
    "@types/node": "^12.12.21",
    "@types/sinon": "^7.5.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "ts-mocha": "^6.0.0",
    "typemoq": "^2.1.0",
    "node-mocks-http": "^1.8.1",
    "npm-check-updates": "^4.0.1",
    "nyc": "^14.1.1",
    "cross-env": "^6.0.3",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "sinon": "^8.0.1"
  }
}
